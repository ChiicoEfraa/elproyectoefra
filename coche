        <combustible>Gasolina</combustible>
        <transmision>Manual</transmision>
        <kilometraje><![CDATA[<b>25000 km</b>]]></kilometraje>
        <!-- EL CDATA de HTML no lo tengo tan claro pero lo he dejado en km por ejemplo en cada uno de los
        coches con la etiqueta <b></b> para que se muestre en negrita como una descripcion debajo de cada coche
        tengo la sesacion o la idea que tambien puede funcionar como un enlace dependiendo la etiqueta que usemos
        de HTML en alguna app como amazon por ejemplo para ir a la tienda del vendedor pinchando en el texto -->
        <revisiones>
            <primera_revision>2020-01-15</primera_revision>
            <ultima_revision>2021-01-15</ultima_revision>
            
        </revisiones>
    </coche>

