<!DOCTYPE coches [
    <!ELEMENT coches (coche+)>
    <!ELEMENT coche (marca, modelo, anio, color, precio, potencia, combustible, transmision, kilometraje, revisiones)>
    <!ELEMENT marca (#PCDATA)>
    <!ELEMENT modelo (#PCDATA)>
    <!ELEMENT anio (#PCDATA)>
    <!ELEMENT color (#PCDATA)>
    <!ELEMENT precio (#PCDATA)>
    <!ELEMENT potencia (#PCDATA)>
    <!ELEMENT combustible (#PCDATA)>
    <!ELEMENT transmision (#PCDATA)>
    <!ELEMENT kilometraje (#PCDATA)>
    <!ELEMENT revisiones (primera_revision, ultima_revision)>
    <!ELEMENT primera_revision (#PCDATA)>
    <!ELEMENT ultima_revision (#PCDATA)>
    <!ATTLIST coche id CDATA #REQUIRED>
    <!ATTLIST precio moneda CDATA #REQUIRED>           
]>

<coches>

    <!-- Cada coche tiene un atributo único llamado id con numeros del 1 al 15 -->
    <coche id="coche1">
        <marca>Ford</marca>
        <modelo>Focus</modelo>
        <anio>2020</anio>
        <color>Rojo</color>
        <precio moneda="EUR">20000</precio>
